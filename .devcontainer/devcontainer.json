{
    "image": "mcr.microsoft.com/devcontainers/base:ubuntu",
    "onCreateCommand": "sh /workspaces/jdk-update/.devcontainer/create.sh",
    "customizations": {
        "vscode": {
            "extensions": [
                "vscjava.vscode-java-pack"
            ]
        }
    },
    "containerEnv":{
        "JAVA_HOME":"/usr/lib/jvm/msopenjdk-21-amd64"
    }
}
